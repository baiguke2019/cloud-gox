{
  "name": "cloud-gox",
  "website": "https://github.com/jpillora/cloud-gox",
  "repository": "https://github.com/jpillora/cloud-gox",
  "env": {
    "GO_INSTALL_TOOLS_IN_IMAGE": {
      "description": "Installs the go toolchain in the final Heroku slug. Must be 'true'",
      "value": "false",
      "required": true
    },
    "GO_SETUP_GOPATH_IN_IMAGE": {
      "description": "Sets the $GOPATH, aswell as other things. Must be 'true'",
      "value": "false",
      "required": true
    },
    "HTTP_USER": {
      "description": "HTTP authentication user",
      "required": false
    },
    "HTTP_PASS": {
      "description": "HTTP authentication password",
      "required": false
    },
    "GH_USER": {
      "description": "Github user (requires password)",
      "required": false
    },
    "GH_PASS": {
      "description": "Github password (requires user)",
      "required": false
    },
    "GH_TOKEN": {
      "description": "Github API token",
      "required": false
    },
    "GH_PAN": {
      "description": "Github Personal Access Token (must have full access to the 'repos' section). Used to clone private repositories",
      "required": false
    }
  }
}
